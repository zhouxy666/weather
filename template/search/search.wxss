.search{
    display: flex;
    flex-direction: row;
    padding:10rpx;
    background: #fff;
    border-bottom: .5px solid #f5f5f5;
    width:100%;
}
.search-icon{
    margin:10rpx 10rpx 0 10rpx;
}
.search input{
    /*border:.3px solid #d1d1d1;*/
    background: #f5f5ff;
    border-radius: 5px;
    width:100%;
    padding-left:10rpx;
    font-size: 24rpx;
    color:#999;
}
.placeholder{
    font-size:12px;
    color:#999;
    font-family: 'Arial';
}
.cancel-icon{
    margin:10rpx 10rpx 0 10rpx;
}